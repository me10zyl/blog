<a href="<%- url_for(post.path) %>" class="<%= class_name %>">
  <time datetime="<%= date_xml(post.date) %>" itemprop="datePublished">
<% var slug_date = post.slug.substring(0, 10) %> 
<% var d = date(post.date, date_format) %>
<% if(slug_date.length == 10 ){ %>
<% 	d = slug_date %>
<% } %>
<%= d %></time>
</a>
